    %meta{ content: "yes", name: "apple-mobile-web-app-capable" }
    %title ChiSafePath
    = stylesheet_link_tag "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css", media: "all"
